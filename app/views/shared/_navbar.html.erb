<div class="navbar-container">
  <% if current_user %>
    <div class="nav-items">
      <a class="nav-logo" href= "/diaries">
        <span>Grape</span>
        <span>Vine</span>
      </a>
      <%= link_to "#{current_user.email}", user_path(current_user), class:"nav-link" %>
      <%= link_to "Logout", destroy_user_session_path, class:"nav-link"  %>
    </div>
  <%else%>
    <a class="nav-logo" href="/users/sign_up">
      <span>Grape</span>
      <span>Vine</span>
    </a>
    <% link_to "Sign up", new_user_registration_path, class:"nav-link" %>
    <% link_to "Log in", new_user_session_path, class:"nav-link"  %>
  <%end%>
  
</div>